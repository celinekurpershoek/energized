{
  "name": "energized",
  "version": "1.0.0",
  "description": "Keep track of your energy usage",
  "main": "index.js",
  "scripts": {
    "develop": "concurrently -r \"npm:type-check:watch\" \"npm:start\"",
    "start": "parcel ./src/index.html",
    "build": "npm run type-check && parcel build ./src/index.html",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "test": "jest",
    "lint": "eslint src/app.tsx",
    "test:watch": "jest --watch",
    "tsc": "./node_modules/typescript/bin/tsc",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettify && npm run lint",
      "pre-push": "npm run test"
    }
  },
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/config/test-preprocessor.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testRegex": [
      "((/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$)"
    ],
    "verbose": true,
    "collectCoverage": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/types": "^7.3.4",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@typescript-eslint/parser": "^1.4.2",
    "concurrently": "^4.1.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.3.1",
    "moment": "^2.24.0",
    "parcel-bundler": "^1.12.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.4",
    "react-testing-library": "^6.0.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "firebase": "^5.8.4",
    "react-moment": "^0.8.4"
  }
}
